{
    "name": "ramsey/uuid",
    "description": "A PHP 5.6+ mathematics library, including support for large numbers.",
    "type": "library",
    "keywords": ["math", "number", "bcmath", "gmp", "arithmetic"],
    "homepage": "https://github.com/ramsey/math",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Ramsey",
            "email": "ben@benramsey.com",
            "homepage": "https://benramsey.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ramsey/math/issues",
        "source": "https://github.com/ramsey/math"
    },
    "require": {
        "php": ">=5.6"
    },
    "suggest": {
        "ext-bcmath": "If using the bcmath functionality",
        "ext-gmp": "If using the gmp functionality"
    },
    "autoload": {
        "psr-4": {
            "Ramsey\\Math\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ramsey\\Math\\Test\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "jakub-onderka/php-parallel-lint": "^0.9.0",
        "satooshi/php-coveralls": "^0.6.1"
    }
}
